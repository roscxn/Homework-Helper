<%- include('../partials/header') %>

    <h1>My Posts</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Question:</th>
                <th>Subject</th>
                <th>Level</th>
            </tr>
        </thead>
        <tbody>
            <% posts.forEach(p=> { %>
                <tr>
                    <td>
                        <%= p.topic %>
                    </td>
                    <td>
                        <%= p.subject %>
                    </td>
                    <td>
                        <%= p.level %>
                    </td>
                </tr>
        </tbody>

        <% }) %>
    </table>

    <%- include('../partials/footer') %>